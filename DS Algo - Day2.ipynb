   "source": [
    "#Given a array,find all the pairs whose sum equal to given K\n",
    "import itertools\n",
    "lst=[1, 5, 7, 8, 2, 0]\n",
    "k=7\n",
    "res_list = [i for i in lst if i <= k]\n",
    "\n",
    "for n in itertools.combinations(res_list, 2):\n",
    "    if n[0] + n[1] == k:\n",
    "        print (str(n[0]) + \" , \" + str(n[1]))"
   ]
